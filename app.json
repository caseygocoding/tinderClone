{
  "expo": {
    "name": "tinder",
    "description": "This project is really great.",
    "slug": "tinder",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/petder-icon.png",
    "splash": {
      "image": "./assets/petder-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.caseychan.petder",
      "supportsTablet": true
    },
    "android": {
      "package": "com.tinder.tinder"
    }
  }
}
